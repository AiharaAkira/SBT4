
public class Test {

	public static void main(String[] args) {
		// 자기소개
		
		//콘솔창 (표준출력공간)에 출력
		
		//System.out 	: 표준 출력공간
		//System.in		: 표준입력공간
		//println: 출력후 줄바꿈
		//print: 출력후 줄바꿈 x
		//printf: 출력 형식 잡을 때 사용
		
		System.out.print("하나");
		System.out.print("둘");
		System.out.println("셋");
		
		// 콘솔에 다음을 출력
		System.out.println("이름 	: 최민우입니다.");
		System.out.println("나이 	: 24살입니다.");
		System.out.println("사는곳	: 의정부입니다.");
		// 가능하면 : 을 정렬 해보세요.
	}
}
