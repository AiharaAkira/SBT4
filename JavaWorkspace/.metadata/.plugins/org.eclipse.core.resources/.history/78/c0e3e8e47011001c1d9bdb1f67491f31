
public class RS_Practice2 {

	public static void main(String[] args) {
		// 달력 가로 출력
		int j =1;

		for (int i = 1; i <= 31; i++) {
				

			for (j = 1; j <= 12; j++) {
				
				
				
				
				
				System.out.printf("%d월", i);
			}
			System.out.printf("%d일\t\t||", i);
			
			System.out.println();
			
		}

		
	}

}
