abstract class Car09 {
	int speed;
	String color;

	void upSpeed(int speed) {
		this.speed += speed;
	}

	abstract void work(); 
}



class Sedan09 extends Car09 {
	@Override
	void work() {
		// TODO Auto-generated method stub
		
	}
}

class Truck09 extends Car09 {
	@Override
	void work() {
		// TODO Auto-generated method stub
		
	}
}


	


public class Ex09 {
	public static void main(String[] args) {
		Car09 car = new Car09();//추상클래스라 인스턴스 생성불가
		
		
		
		Sedan08 sedan1 = new Sedan08();
		System.out.println("승용차생성");
		Truck08 truck1 = new Truck08();
		System.out.println("트럭생성");
		
	}
}

