
public class Practice01 {
	
	public static void main(String[] args) {
		Animal dog = new Animal();
		
		dog.type= "강아지";
		dog.age = 8;
		System.out.print(dog.type + "가 ");
		dog.move();
		
Animal cat = new Animal();
		
		cat.type= "고양이";
		cat.age = 18;
		System.out.print(cat.type + "가 ");
		cat.move();
		
		System.out.print(dog.type + "는 " + dog.age + "개월");
		
		System.out.print(cat.type + "는 " + cat.age + "개월");
	}
			
}
