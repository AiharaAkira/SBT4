
public class RS_Practice2 {

	public static void main(String[] args) {
		// 달력 가로 출력
		int j =1;

		for (int i = 1; i <= 31; i++) {
				

			for (j = 1; j <= 12; j++) {
				if(i>=30 && j==12) {
					break;
				}else if(i>=30 && j==11){
					break;
				}
				
				
				
				
				System.out.printf("%d월%d일\t\t||", j, i);
			}
			
			System.out.println();
			
		}

		
	}

}
