
public class Goods {

	String productName;
	int price;
	public final static String production = "애플";

	public Goods() {

	}

	public Goods(String productName, int price) {
		super();
		this.productName = productName;
		this.price = price;

	}
	
	public void info() {
		System.out.println("품명 : " + productName);
		System.out.println("가격 : " + price);
		System.out.println("제조사 : " + production);
	}

}
