
public class CDMain2 {

	public static void main(String[] args) throws InterruptedException {
		// TODO Auto-generated method stub
		for(int i = 1; i <= 6; i+=2) {
			System.out.println("i= "+i);
		}
	
		for(int i = 0; i <= 8; i+=2) {
			Thread.sleep(1000);
			System.out.println("i= "+i);
			
			
		}
	}
	
	

}
