
public class Person {
	String name;
	String gender;
	
	//기본 생성자 : 생성자를 전혀 안건드리면 자동으로 만들어줌
	// 오버로딩 생성자를 만들었다면 자동으로 안만들어줌
	
	//안 보여도 존재. 근데 '응애' 넣어야 되서 가시화 시킨 것
	public Person() {
		System.out.println("응애");
	}

	public Person(String name, String gender) {
		super();
		this.name = name;
		this.gender = gender;
		System.out.println("응애");
	}
	
	
}
