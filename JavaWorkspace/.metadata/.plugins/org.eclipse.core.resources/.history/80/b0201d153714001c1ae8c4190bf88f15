
public class AMain2 {

	public static void main(String[] args) {
		
		// 학생의 일어점수 90점
		int jpScore = 90;
		System.out.println(jpScore);
		
		//배열 (참조형)
		// 학생 5명의 영어 점수
		int[] engScore = new int[5]; // 5칸
		System.out.println(engScore);
		
		engScore[0] = 100;
		engScore[1] = 90;
		engScore[2] = 80;
		engScore[3] = 70;
		engScore[4] = 60;
	}

}
