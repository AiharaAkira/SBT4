
public class RS_Practice2 {

	public static void main(String[] args) {
		// 달력 가로 출력
		int j = 1;
		int control = 31;

		for (int i = 1; i <= control; i++) {

			for (j = 1; j <= 12; j++) {
				
				if(j ==2) {
					control = 28;
				}else if(j==4 || j == 6|| j ==9|| j == 11){
					control = 30;
				}else {
					continue;
				}

				System.out.printf("%d월%d일\t\t||", j, i);
				
				
			}

			System.out.println();

		}

	}

}
