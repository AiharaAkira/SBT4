
public class RSMain_finall2 {

	public static void main(String[] args) {
		for (int i = 1; i <= 12; i++) {
			for (int j = 1; j <= 31; j++) {
				if(j == 2 && i >= 29) {
					System.out.printf("\t\t");
					continue;
				}else if((j == 4 && i >= 31)||(j == 6 && i >= 31)||(j == 9 && i >= 31)){
					System.out.printf("\t\t");
					continue;
				}else if((j == 11 && i >= 31)) {
					System.out.printf("\t\t\t");
					continue;
				}
				System.out.printf("%d¿ù%dÀÏ||\t\t", i, j);
			}
			System.out.println();
		}

	}

}
