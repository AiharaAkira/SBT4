
public class AMain4 {

	public static void main(String[] args) {
		
		int aa[] = {100, 200, 300, 400};
		
		int bb[] = new int[] {100, 200, 300};
		
		int cc[];
		cc = new int[] {100, 200};
		
		int dd[] = new int[1];
		dd[0] = 100;
		
		
		// aa[0] = 100
		// aa[1] = 200
		// aa[2] = 300
		// aa[3] = 400
		
		for(int i : aa) {
			System.out.println("aa[" +i+ "]" + "=" + aa[i]);
		}
		
	}

}
