
public class RSMain_final {

	public static void main(String[] args) {
		
		for (int i = 1; i <= 31; i++) {
			for (int j = 1; j <= 12; j++) {
				
				if(j == 2 && i == 28) {
					System.out.printf("\t\t");
					continue;
				}
				
				System.out.printf("%d¿ù%dÀÏ||\t\t", j, i);
			}
			System.out.println();
		}

	}

}
