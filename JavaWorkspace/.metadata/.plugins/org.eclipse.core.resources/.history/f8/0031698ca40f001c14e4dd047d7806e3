import java.util.Iterator;

public class RSMain {

	public static void main(String[] args) {
		
		// Repeat Stmt(반복문)
		
		
		/*
		 * for(초기화; 조건; 증감){
		 * 
		 * }
		 */
//		반복횟수가 명확하면 for
		for(int i = 0; i < 5; i ++) {
			
			System.out.println("ㅎㅎ");
			
		}
		
		//반복횟수가 명확하지가 않으면 while
		int z = 0;
		while(z < 5) {
			System.out.println("ㅌㅌ");
		}
		
	}

}
